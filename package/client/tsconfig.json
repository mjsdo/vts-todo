{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./build",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@constants/*": ["src/constants/*"],
      "@core/*": ["src/core/*"],
      "@styles/*": ["src/styles/*"],
      "@utils/*": ["src/utils/*"],
      "@IDB/*": ["src/IDB/*"],
      "@storage/*": ["src/storage/*"],
      "@stores/*": ["src/stores/*"]
    }
  }
}
